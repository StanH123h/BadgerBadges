{
  "name": "badgerbadge",
  "version": "1.0.0",
  "private": true,
  "description": "UW-Madison campus achievements NFT system",
  "scripts": {
    "dev:web": "pnpm --filter web dev",
    "dev:contracts": "pnpm --filter contracts node",
    "build:web": "pnpm --filter web build",
    "build:contracts": "pnpm --filter contracts compile",
    "deploy:local": "pnpm --filter contracts deploy:local",
    "deploy:sepolia": "pnpm --filter contracts deploy:sepolia",
    "test:contracts": "pnpm --filter contracts test",
    "solana:build": "cd packages/solana-program && anchor build",
